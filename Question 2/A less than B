*Q2*
MOV 50H,#05H
MOV 51H, #07H
MOV R0, 50H     
MOV R1, 51H    
LOOP:
    MOV A, R0
    JZ  A_LOOP  
    MOV A, R1
    JZ  B_LOOP  
    DEC R0      
    DEC R1
    SJMP LOOP

A_LOOP: MOV A, R1
    JZ  EQUAL   
    MOV 52H, #0FFH   
    SJMP END_PROG
B_LOOP: MOV 52H, #01H    
    SJMP END_PROG
EQUAL: MOV 52H, #00H    
END_PROG: 
	END
